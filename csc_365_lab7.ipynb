    "def check_revenue(conn):\n",
    "  pd.set_option('display.max_columns', None)\n",
    "  pd.set_option('display.max_rows', None)\n",
    "  with conn.cursor() as cursor:\n",
    "    print(\"Check revenue\")\n",
    "    print(\"----------\")\n",
    "    reservations = pd.read_sql(\"\"\"\n",
    "                                SELECT RoomCode, CheckIn, Checkout, Rate, DATEDIFF(Checkout, CheckIn) AS DaysStayed\n",
    "                                FROM lab7_rooms INNER JOIN lab7_reservations\n",
    "                                  ON lab7_rooms.RoomCode = lab7_reservations.Room\n",
    "                                WHERE YEAR(lab7_reservations.CheckIn) = YEAR(CURDATE())\n",
    "                                ORDER BY RoomCode;\n",
    "                              \"\"\", conn)\n",
    "    \n",
    "    # Convert to dates to  datetime\n",
    "    reservations['CheckIn'] = pd.to_datetime(reservations['CheckIn'])\n",
    "    reservations['Checkout'] = pd.to_datetime(reservations['Checkout'])\n",
    "\n",
    "    # Create a new dataframe where columns are RoomCode, months, Total\n",
    "    months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December']\n",
    "    monthly_rev = pd.DataFrame(0, index=reservations['RoomCode'].unique(), columns=months + ['Total'])\n",
    "\n",
    "    for i, row in reservations.iterrows():\n",
    "      checkin_month = row['CheckIn'].month\n",
    "      checkout_month = row['Checkout'].month\n",
    "      room = row['RoomCode']\n",
    "\n",
    "      if row['DaysStayed'] > 0:\n",
    "        \n",
    "        # end_month + 1 to include the checkout month\n",
    "        for month in range(checkin_month, checkout_month + 1):\n",
    "          month_name = months[month - 1]\n",
    "\n",
    "          # Entire reservation occurred in the same month\n",
    "          if month == checkin_month and month == checkout_month:\n",
    "            # Subtract 1 since checkout day doesn't count\n",
    "            days = (row['Checkout'] - row['CheckIn']).days - 1\n",
    "          # Otherwise reservation spans different months so:\n",
    "          elif month == checkin_month:\n",
    "            # Get the number of days from the checkin date to the end of the checkin month\n",
    "            # Subtract 1 since checkout day doesn't count\n",
    "            days = (pd.Timestamp(f\"{row['CheckIn'].year}-{month + 1}-01\") - row['CheckIn']).days - 1\n",
    "          elif month == checkout_month:\n",
    "            # Get the number of days from beginning of the checkout month to the checkout date\n",
    "            days = (row['Checkout'] - pd.Timestamp(f\"{row['Checkout'].year}-{month}-01\")).days\n",
    "          \n",
    "          # Update the current total revenue of the month for a given room and month\n",
    "          monthly_rev.at[room, month_name] += row['Rate'] * days\n",
    "\n",
    "    # Sum up all the columns of each row\n",
    "    monthly_rev['Total'] = monthly_rev.sum(axis=1)\n",
    "\n",
    "    print(\"Room Revenues:\")\n",
    "    print(monthly_rev)\n",
    "\n",
    "    \n"
